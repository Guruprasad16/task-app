{
    "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ],
    "importOrder": [
      "^react",
      "^@mui",
      "^axios",
      "^redux",
      "^@redux",
      "^@app/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "jsxSingleQuote": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css"
  }